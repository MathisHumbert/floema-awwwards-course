{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "app.js" }],
  "env": {
    "PRISMIC_ENDPOINT": "@prismic_endpoint",
    "PRISMIC_ACCESS_TOKEN": "@prismic_access_token",
    "PRISMIC_CLIENT_ID": "@prismic_client_id",
    "PRISMIC_CLIENT_SECRET": "@prismic_client_secret"
  }
}
